using System;

namespace Switch
{
    class Program
    {
        public static void Main(string[] args)
        {
            char ch;

            Console.WriteLine("Enter an Alphabet");
            ch = Convert.ToChar(Console.ReadLine());

            switch (Char.ToLower(ch))
            {
                case 'a':
                case 'e':
                case 'i':
                case 'o':
                case 'u':
                    Console.WriteLine("vowel");
                    break;
                default:
                    Console.WriteLine("not vowel");
                    break;
            }

            Console.ReadKey();
        }
    }
}
